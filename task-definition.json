{
  "family": "thendy-task",
  "networkMode": "awsvpc",
  "containerDefinitions": [
    {
      "name": "trendy",
      "image": "211125776553.dkr.ecr.ap-northeast-2.amazonaws.com/trendy-backend:latest",
      "cpu": 512,
      "memory": 3072,
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 8081,  
          "protocol": "tcp"
        }
      ]
    }
  ],
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "3072"
}
